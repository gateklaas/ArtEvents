    <time:hasEnd rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-05-17T15:00:00Z</time:hasEnd>
    <time:hasBeginning rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-02-12T09:00:00Z</time:hasBeginning>
    <dbo:location rdf:resource="http://dbpedia.org/resource/Rijksmuseum"/>
    <dbo:cost rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">7.5</dbo:cost>
    <time:hasEnd rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-05-16T22:00:00Z</time:hasEnd>
    <time:hasBeginning rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-02-11T23:00:00Z</time:hasBeginning>
    <dbo:abstract xml:lang="nl">&lt;p&gt;Overzicht van het werk van Rembrandt van Rijn&amp;nbsp;tussen ca. 1652 en zijn dood in 1669. De schilderijen die Rembrandt&amp;nbsp;in deze jaren maakte zijn nog steeds bepalend voor het Rembrandtbeeld wereldwijd.&lt;/p&gt; &lt;p&gt;Rembrandt experimenteerde met grafiek- en schildertechnieken en wist een ongekende diepgang in zijn werk te brengen. De schilderijen en tekeningen komen uit vooraanstaande musea en particuliere collecties uit Europa en de Verenigde Staten.&lt;/p&gt;</dbo:abstract>
    <foaf:name xml:lang="nl">De late Rembrandt</foaf:name>
  </owl:Thing>
  <owl:Thing rdf:about="http://data.artsholland.com/event/4d4b3276-ec07-4ed0-8cdf-cbc0d9db7025">
    <rdf:type rdf:resource="http://www.semanticweb.org/Finalassessment#Event"/>
    <rdf:type rdf:resource="&owl;NamedIndividual"/>
