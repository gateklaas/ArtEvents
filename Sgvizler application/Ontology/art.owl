        <rdfs:label xml:lang="de">Die schwimmende Feder</rdfs:label>
        <rdfs:label xml:lang="en">The Floating Feather</rdfs:label>
        <dbp:widthMetric rdf:datatype="&xsd;integer">144</dbp:widthMetric>
